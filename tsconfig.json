    "target": "esnext",
    "outDir": "./dist/js/",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "lib": ["esnext", "dom"],
  "include": [
    "./src/js/**/*.ts"
  ],
  "exclude": ["dist", "node_modules"]
}
